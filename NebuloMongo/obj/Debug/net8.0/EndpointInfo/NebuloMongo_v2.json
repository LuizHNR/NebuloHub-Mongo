          "rating": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "string",
            "nullable": true
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "startupId": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RequestStartupDto": {
        "type": "object",
        "properties": {
          "cnpj": {
            "type": "string",
            "nullable": true,
            "example": "12345678000199"
          },
          "video": {
            "type": "string",
            "nullable": true,
            "example": "https://www.youtube.com/watch?v=abcd1234"
          },
          "name": {
            "type": "string",
            "nullable": true,
            "example": "Nebulo Corp"
          },
          "descricao": {
            "type": "string",
            "nullable": true
          },
          "site": {
            "type": "string",
            "nullable": true,
            "example": "https://nebulocorp.com"
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time",
            "example": "2025-11-22T14:30:00Z"
          },
          "habilidades": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true,
            "example": [
              "IA",
              "Machine Learning",
              "Análise de Dados"
            ]
          },
          "idUser": {
            "type": "string",
            "nullable": true,
            "example": "64ena91b92b122bd1acf9f77"
          }
        },
        "additionalProperties": false
      },
